extends layout

block content
  h1= title
  p Welcome to #{title}
  Welcome to #{ok}
  Welcome to #{4 *5}
  form(action="." method="POST")
  label(for="name") Name:
  input(
    type="text"
    id="name"
    name="name"
  )

  label(for="email") Email:
  input(
    type="email"
    id="email"
    name="email"
  )

  input(type="submit" value="Submit")

